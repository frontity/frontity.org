{
  "name": "frontity-org",
  "version": "1.0.0",
  "private": true,
  "description": "Frontity project",
  "keywords": [
    "frontity"
  ],
  "scripts": {
    "dev": "frontity dev",
    "dev:inspect": "node --inspect -r ts-node/register/transpile-only ./node_modules/.bin/frontity dev",
    "build": "frontity build",
    "serve": "frontity serve",
    "gc:build": "gcloud builds submit --tag gcr.io/frontity-org/frontity-org",
    "gc:deploy": "gcloud run deploy --image gcr.io/frontity-org/frontity-org --platform managed"
  },
  "prettier": {},
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@changesets/changelog-github": "^0.2.7",
    "@changesets/cli": "^2.11.2",
    "@frontity/core": "^1.9.1",
    "@frontity/frontity-org-theme": "file:packages/frontity-org-theme",
    "@frontity/google-tag-manager-analytics": "^1.1.1",
    "@frontity/html2react": "^1.5.0",
    "@frontity/tiny-router": "^1.2.3",
    "@frontity/twentytwenty-theme": "file:packages/twentytwenty-theme",
    "@frontity/wp-source": "^1.10.0",
    "@frontity/yoast": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "eslint": "^7.13.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "frontity": "^1.13.0",
    "frontity-contact-form-7": "file:packages/frontity-contact-form-7",
    "husky": "^4.3.0",
    "intersection-observer": "^0.11.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2"
  }
}
